<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guesski.controller.MenuController">
    <stylesheets>
        <URL value="@styleSheet/Menu.css" />
    </stylesheets>
    <children>
      <HBox alignment="CENTER" fx:id="hbox">
         <children>
            <VBox alignment="CENTER">
      
                <children>
                  <Label text="GUESSKI" />
                  <Button mnemonicParsing="false" onAction="#openGame" text="JOUER" />
                  <Button mnemonicParsing="false" onAction="#openHelp" text="AIDE" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
